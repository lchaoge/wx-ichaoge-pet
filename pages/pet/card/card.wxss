page{
  background: #fedb1d;
}
.m-header{
  background: #fedb1d;
}
.i-cell{
  background: #fedb1d;
}
.card-panel{
  padding: 10px;
}
.card-add{
  background-color:#fff;
  overflow: hidden;
  padding: 5vh 10px;
  border-radius: 5px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  position: relative;
}
.card-add .title{
  font-size: 16px;
}
.card-add .desc{
  font-size: 12px;
  color: #999;
}
.card-add .image{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 140px;
  height: 52px;
}
.card-add .image image{
  width: 100%;
  height: 100%;
  display: block;
}
.i-cell-text{
  height: 26px;
  line-height: 20px;
}
.i-cell-text .title{
  font-size: 16px;
}
.i-cell-text .shitOfficer{
  line-height: 3;
  padding: 0 2px;
  font-size: 6px;
  border-radius: 10px;
  border: 1px solid #000;
  vertical-align: super;
  margin-left: 10px;
}
/* 列表 */
.home-panel-userinfo{
  position: relative;
  background: #fff;
  padding: 10px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
  border-radius: 5px;
  overflow: hidden;
  margin-bottom: 10px;
}
.userinfo-left .title{
  font-size: 18px;
  margin-bottom: 10px;
}
.userinfo-left .attribute{
  padding-right: 5px;
  overflow: hidden;
  height: 30px;
  margin-bottom: 15px;
  font-size: 12px;
  color: #999;
}
.userinfo-left .attribute .sex.female{
  background: #f8807f;
}
.userinfo-left .attribute .sex.male{
  background: #75a0ff;
}
.userinfo-left .attribute .sex{
  overflow: hidden;
  border-radius: 22px;
  float: left;
  line-height: 22px;
  height:22px;
  padding-left: 22px;
  padding-right: 10px;
  position: relative;
  margin-right: 10px;
}
.userinfo-left .attribute .sex image{
  position: absolute;
  top: 4px;
  left: 4px;
  width: 14px;
  height:14px;
  display: block;
  z-index: 1;
}
.userinfo-left .attribute .sex text{
  color: #fff;
  font-size: 12px;
}
.userinfo-left .attribute .birthDate{
  display: block;
  float: left;
  line-height: 22px;
  height:22px;
  font-size: 12px;
}
.userinfo-left .list .item{
  line-height: 20px;
  height: 25px;
  font-size: 12px;
  color: #999;
}
.userinfo-left .list .item image{
  vertical-align: middle;
  width: 16px;
  height: 16px;
  margin-right: 5px;
}
.userinfo-left .list .item text{
  vertical-align: middle;
}
.userinfo-right .petImage{
  position: absolute;
  top: 10px;
  right: 10px;
  width: 80px;
  height: 80px;
  border-radius: 50%;
  overflow: hidden;
}
.userinfo-right .petNo{
  position: absolute;
  bottom: 10px;
  right: 10px;
  font-size: 12px;
  color: #999;
}